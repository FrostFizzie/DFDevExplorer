{
  "schemaVersion": 1,
  "id": "dfdevexplorer",
  "version": "${version}",
  "name": "DFDevExplorer",
  "description": "Adds the developer nodes on mcdiamondfire via the node selector menu allowing you a visual way to join the dev nodes if you have access.",
  "authors": ["FrostFizzie"],
  "contact": {},
  "license": "MIT",
  "icon": "assets/DFDevExplorer/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "org.FrostFizzie.DFDevExplorer.client.DFDevExplorerClient"
    ],
    "main": [
      "org.FrostFizzie.DFDevExplorer.DFDevExplorer"
    ]
  },
  "mixins": [
    "DFDevExplorer.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
